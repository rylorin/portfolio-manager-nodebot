{
  "name": "nodebot",
  "repository": {
    "url": "https://github.com/rylorin/nodebot"
  },
  "license": "UNLICENSED",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist/",
    "register/",
    "LICENSE"
  ],
  "scripts": {
    "build": "yarn run server:build && yarn run client:build && yarn run storybook:build",
    "build:ib": "cd ./node_modules/@stoqey/ib && yarn && yarn build",
    "build:watch": "yarn tsc --watch",
    "clean": "rm -rf dist",
    "client:build": "vite build",
    "client:dev": "vite",
    "dev": "concurrently \"yarn run server:dev\" \"yarn run client:dev\"",
    "doc": "typedoc --excludeInternal",
    "doc:dev": "typedoc --out ./doc-dev",
    "postinstall": "patch-package",
    "lint": "eslint . --ext .ts,.tsx",
    "prepare": "husky install",
    "prepublishOnly": "yarn build",
    "qc": "yarn lint && tsc --noEmit && prettier --check . && stylelint \"**/*.css\"",
    "release": "yarn lint && yarn qc && yarn doc && yarn build",
    "serve": "vite preview",
    "server:build": "tsc --project tsconfig.server.json",
    "server:dev": "ts-node-dev --project tsconfig.server.json src/index -server -import",
    "start": "node dist/index.js -server",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build -o dist/storybook"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.0",
    "@chakra-ui/layout": "^2.3.0",
    "@chakra-ui/react": "^2.8.0",
    "@chakra-ui/system": "^2.6.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@stoqey/ib": "^1.3.0",
    "chakra-react-select": "^4.7.0",
    "chart.js": "^4.3.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fast-xml-parser": "^4.2.7",
    "formik": "^2.4.3",
    "framer-motion": "^10.15.0",
    "json-stringify-safe": "^5.0.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.2",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.32.1",
    "sequelize-typescript": "^2.1.3",
    "sqlite3": "^5.1.6",
    "winston": "^3.10.0",
    "yahoo-finance2": "^2.4.3"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@chakra-ui/storybook-addon": "^5.0.1",
    "@storybook/addon-actions": "^7.2.1",
    "@storybook/addon-essentials": "^7.2.1",
    "@storybook/addon-links": "^7.2.1",
    "@storybook/cli": "^7.2.1",
    "@storybook/components": "^7.2.1",
    "@storybook/react": "^7.2.1",
    "@storybook/react-vite": "^7.2.1",
    "@storybook/testing-library": "^0.2.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^18.17.2",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@types/react-router-dom": "^5.3.3",
    "@types/validator": "^13.9.0",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "@vitejs/plugin-react": "^4.0.4",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-rxjs": "^5.0.3",
    "eslint-plugin-storybook": "^0.6.13",
    "http-proxy-middleware": "^2.0.6",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "patch-package": "^8.0.0",
    "prettier": "^3.0.1",
    "rxjs": "^7.8.1",
    "storybook": "^7.2.1",
    "stylelint": "^15.10.2",
    "stylelint-config-standard": "^34.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.8",
    "webpack": "^5.88.2"
  },
  "engines": {
    "node": ">=18.17.0"
  }
}
