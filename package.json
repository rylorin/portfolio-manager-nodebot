{
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist/",
    "register/",
    "LICENSE"
  ],
  "license": "MIT",
  "scripts": {
    "lint": "./node_modules/.bin/eslint . --ext .ts",
    "test": "jest --maxWorkers=8 --reporters=default --useStderr --detectOpenHandles",
    "test-next": "jest ./src/tests/unit/api-next/ --maxWorkers=8 --reporters=default --useStderr --detectOpenHandles",
    "clean": "rm -rf dist",
    "build": "yarn tsc",
    "build:watch": "yarn tsc --watch",
    "prepublishOnly": "yarn build",
    "release": "yarn lint && yarn test && yarn doc && yarn build",
    "doc": "typedoc --excludeInternal",
    "doc:dev": "typedoc --out ./doc-dev",
    "tsc": "./node_modules/.bin/tsc",
    "start": "node ./dist/index.js -watch -clientId=1 -log=warn",
    "startX": "node ./dist/index.js -watch -clientId=0 -log=warn -updater -account -roll"
  },
  "engines": {
    "node": ">=4.2.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "typescript": "^4.2.4",
    "eslint": "^7.24.0"
  },
  "dependencies": {
    "@stoqey/ib": "^1.2.25",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.17.0",
    "sequelize-typescript": "^2.1.3",
    "sqlite3": "git@github.com:mapbox/node-sqlite3.git#3fb3715c4e699ca3bc19e47b136758c66f61e477"
  }
}