{
  "name": "nodebot",
  "repository": {
    "url": "https://github.com/rylorin/nodebot"
  },
  "license": "UNLICENSED",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist/",
    "register/",
    "LICENSE"
  ],
  "scripts": {
    "build": "yarn run server:build && yarn run client:build && yarn run storybook:build",
    "build:ib": "cd ./node_modules/@stoqey/ib && yarn && yarn build",
    "build:watch": "yarn tsc --watch",
    "clean": "rm -rf dist",
    "client:build": "vite build",
    "client:dev": "vite",
    "dev": "concurrently \"yarn run server:dev\" \"yarn run client:dev\"",
    "doc": "typedoc --excludeInternal",
    "doc:dev": "typedoc --out ./doc-dev",
    "postinstall": "patch-package",
    "lint": "./node_modules/.bin/eslint . --ext .ts",
    "prepare": "husky install",
    "prepublishOnly": "yarn build",
    "release": "yarn lint && yarn test && yarn doc && yarn build",
    "serve": "vite preview",
    "server:build": "tsc --project tsconfig.server.json",
    "server:dev": "ts-node-dev --project tsconfig.server.json src/index -server -import",
    "start": "node dist/index.js -server",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build -o dist/storybook",
    "test": "eslint . && tsc --noEmit && prettier --check . && stylelint \"**/*.css\"",
    "tsc": "./node_modules/.bin/tsc"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.19",
    "@chakra-ui/layout": "^2.2.0",
    "@chakra-ui/react": "^2.7.1",
    "@chakra-ui/system": "^2.5.8",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@stoqey/ib": "^1.2.39",
    "chart.js": "^4.3.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fast-xml-parser": "^4.2.5",
    "framer-motion": "^10.12.17",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.32.1",
    "sequelize-typescript": "^2.1.3",
    "sqlite3": "^5.1.6",
    "yahoo-finance2": "^2.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@chakra-ui/storybook-addon": "^5.0.0",
    "@storybook/addon-actions": "^7.0.23",
    "@storybook/addon-essentials": "^7.0.23",
    "@storybook/addon-links": "^7.0.23",
    "@storybook/cli": "^7.0.23",
    "@storybook/react": "^7.0.23",
    "@storybook/react-vite": "^7.0.23",
    "@storybook/testing-library": "^0.2.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.1",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/validator": "^13.7.17",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "@vitejs/plugin-react": "^4.0.1",
    "babel-loader": "^9.1.2",
    "concurrently": "^8.2.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-rxjs": "^5.0.3",
    "eslint-plugin-storybook": "^0.6.12",
    "http-proxy-middleware": "^2.0.6",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "patch-package": "^7.0.0",
    "prettier": "2.8.8",
    "rxjs": "^7.8.1",
    "storybook": "^7.0.23",
    "stylelint": "^15.9.0",
    "stylelint-config-standard": "^33.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "webpack": "^5.88.0"
  },
  "engines": {
    "node": ">=18.16.0"
  }
}
